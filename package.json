{
  "name": "@incodelang/editor",
  "version": "4.0.13",
  "description": "The next generation of the InCode Editor",
  "main": "",
  "scripts": {
    "webpack:build-dev": "webpack --mode development",
    "webpack:build-prod": "webpack --mode production",
    "express:dev-start": "yarn webpack:build-dev && node ./src/backend/server.js",
    "express:prod-start": "yarn webpack:build-dev && node ./src/backend/server.js",
    "express:only-start": "node ./src/backend/server.js",
    "express:live-start": "yarn concurrently \"yarn webpack --watch\" \"yarn express:only-start\"",
    "express:live-start:prod": "yarn concurrently \"yarn webpack --watch --mode production\" \"yarn express:only-start\"",
    "lint": "yarn eslint --ext .ts,.tsx,.js",
    "prettier:check": "yarn prettier --check src/**/*.{ts,tsx,js}",
    "prettier:write": "yarn prettier --write src/**/*.{ts,tsx,js}",
    "docker:build": "docker build . -t\"mctzock/incode-editor\"",
    "docker:deploy": "docker push mctzock/incode-editor",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/InCodeDevs/Editor.git"
  },
  "keywords": [
    "Editor",
    "InCode",
    "InCode",
    "Language",
    "Programming"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "The InCode Developers",
  "license": "GPL-3.0-only",
  "bugs": {
    "url": "https://github.com/InCodeDevs/Editor/issues"
  },
  "homepage": "https://github.com/InCodeDevs/Editor#readme",
  "devDependencies": {
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "concurrently": "^6.4.0",
    "copy-webpack-plugin": "^10.0.0",
    "css-loader": "^6.5.1",
    "eslint": "^8.3.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "node-sass": "^6.0.1",
    "prettier": "^2.5.0",
    "sass-loader": "^12.3.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "@babel/plugin-transform-react-jsx": "^7.16.5",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.16",
    "@incodelang/accounts": "^1.0.18",
    "@incodelang/accounts-client": "^1.0.11",
    "@incodelang/templates": "^1.0.2",
    "@incodelang/urlshorter": "^1.0.2",
    "@mdx-js/loader": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@monaco-editor/react": "^4.3.1",
    "@types/mdx": "^2.0.1",
    "@types/mdx-js__react": "^1.5.5",
    "@types/react-router-dom": "^5.3.2",
    "babel-loader": "^8.2.3",
    "blockly": "^6.20210701.0",
    "body-parser": "^1.19.0",
    "chalk": "^4.1.2",
