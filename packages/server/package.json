{
  "name": "@incodelang/server",
  "version": "4.0.0",
  "description": "",
  "author": "Ben Siebert <dev@mctzock.de>",
  "homepage": "https://incodelang.de",
  "license": "GPL-3.0-only",
  "main": "src/index.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/InCodeDevs/Editor.git"
  },
  "scripts": {
    "express:dev-start": "lerna run --scope=@incodelang/app webpack:build-dev && node ./src/index.js",
    "express:prod-start": "lerna run --scope=@incodelang/app webpack:build-prod && node ./src/index.js",
    "express:only-start": "forever ./src/index.js",
    "express:live-start": "concurrently \"lerna run webpack:watch-dev\" \"yarn express:only-start\"",
    "express:live-start:prod": "yarn concurrently \"lerna run --scope=@incodelang/app webpack:watch-dev\" \"yarn express:only-start\""
  },
  "bugs": {
    "url": "https://github.com/InCodeDevs/Editor/issues"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "chalk": "^4.1.2",
    "concurrently": "^6.4.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "forever": "^4.0.3",
    "kill-port": "^1.6.1",
    "lerna": "^4.0.0",
    "moment": "^2.29.1",
    "serve-index": "^1.9.1",
    "simple-git": "^3.2.6",
    "web-push": "^3.4.5"
  }
}
